{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "repo-manage",
  "mainBinaryName": "repo-manage",
  "version": "0.0.0",
  "identifier": "com.lengtouzai.repo-manage",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "height": 800,
        "width": 1000,
        "resizable": true,
        "decorations": false,
        "transparent": true,
        "title": "repo-manage",
        "useHttpsScheme": true
      }
    ]
  }
}